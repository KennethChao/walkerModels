function dx = dymModelStanceSLIPPendulum(t, x, parms)
% Get parameters
g = parms.g;
k = parms.k;
mf = parms.mf;
% Get state variables
l = x(1);
ld = x(2);
theta = x(3);
thetad = x(4);
phi = x(5);
phid = x(6);



% Evalute dimensionless EOMs of the SLIP model
b = zeros(3,1);

b(1) = -(1+mf)*g*sin(theta) + (1+mf)*l*thetad^2-k*(l-1)-rc*phid*thetad*cos(phi-theta);
b(2) = -(1+mf)*g*sin(theta) + (1+mf)*l*thetad^2-k*(l-1)-rc*phid*thetad*cos(phi-theta);




dx = [ld; ldd; thetad; thetadd; phid; phidd];

end
clc;
clear B;

A = solution;

B = nan(2,size(solution,2));
for i = 1:size(solution,2)
buf =solution(:,i);
buf(~isnan(buf))
uniqeBuf = uniquetol(buf(~isnan(buf)),1e-2)
if ~isempty(uniqeBuf)
B(1:length(uniqeBuf),i) =uniqeBuf;
end


combinedDelta = [B(1,:) fliplr(B(2,:))];
combinedKvec = [B(1,:) fliplr(B(2,:))];

end